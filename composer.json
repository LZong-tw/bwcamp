{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3 | ^8.0",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "doctrine/dbal": "^2.10",
        "dompdf/dompdf": "0.8.6.3",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "laravel/framework": "^8.83",
        "laravel/telescope": "^4.7",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "milon/barcode": "^8.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "sentry/sentry-laravel": "^2.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/helo-laravel": "^1.3",
        "dietercoopman/laravel-showsql": "^1.3",
        "facade/ignition": "^2.5",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vladislavtkachenko/dompdf.git",
            "canonical": false
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "studio/laravel-totem",
                "barryvdh/laravel-ide-helper",
                "beyondcode/helo-laravel"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "vendor/laravel/framework/src/Illuminate/Queue/Jobs/Job.php",
            "vendor/beyondcode/helo-laravel/src/HeloLaravelServiceProvider.php"
        ],
        "files": [
            "app/Overrides/Job.php",
            "app/Overrides/HeloLaravelServiceProvider.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-install-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
